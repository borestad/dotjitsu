#!/usr/bin/env bash

cd "$(dirname $0)"/..

echo ''; hr - '#' -
echo "[CLEANING]"
echo

dfc
echo

# Kill log files slowing down the shell
find /private/var/log/asl -name '*.asl' | sudo xargs rm -rfv

rm -rfv ~/.v8flags*

chmod -R +x ~/.dotjitsu/bin/*

brew cleanup -s --verbose
brew prune --verbose

echo
dfc

