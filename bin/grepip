#!/usr/bin/env bash
set -m

# export IP_SIMPLE="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}"
export IS_PRIVATE="(#|0|10\.|100\.|172\.16|127\.|192\.168|100\.64|224\.0|169\.254|255\.|240\.)"
export IS_IP4='(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)'
# export IS_CIDR4="$IS_IP4(/([0-9]|[1-2][0-9]|3[0-2]))?"
export IS_CIDR4="$IS_IP4(?:\/\d{1,2})?"
export IS_URL='(https?|ftp|file)://[-A-Za-z0-9\+&@#/%?=~_|!:,.;]*[-A-Za-z0-9\+&@#/%=~_|]\.[-A-Za-z0-9\+&@#/%?=~_|!:,.;]*[-A-Za-z0-9\+&@#/%=~_|]'


NOT_ENDS_WITH='.*[^\.]'

if [ -n "$1" ]; then
    cat "$1"
else
    cat
fi | rg --pcre2 --only-matching "${IS_CIDR4}"      # egrep -h "$IS_CIDR"
