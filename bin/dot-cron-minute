#!/usr/bin/env bash
set -m

source ~/.dotjitsu/.env$
export TERM='xterm-256color'
LOG_LOCATION=~/logs/cron/minute/`date +%F`
TERM="xterm-256color"

mkdir -p $LOG_LOCATION

exec > >(tee -i $LOG_LOCATION/cron-minute-`date +%F--%H%M%S`.log)
exec 2>&1

dot-rsync

echo $PATH
echo "All done! Enjoy a cold one! 🍺 "
